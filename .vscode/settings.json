{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,

  "tailwindCSS.includeLanguages": {
    "vue": "html",
    "vue-html": "html"
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true
}
